		$unit2 = $get['nm_skpd'];

		$qry = "select nm_skpd from ref_skpd_baru where c1='".$isi['c1']."' and c2='".$isi['c2']."' and d2='".$isi['d2']."' and e2='".$isi['e2']."' and e12='".$isi['e12']."'";
