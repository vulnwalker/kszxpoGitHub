		$UID = $_COOKIE['coID'];
		$cek = ''; $err=''; $content=''; $json=TRUE;

		if($err == ''){
			for($i = 0; $i<count($ids); $i++){

				//Batalkan SOTK Baru
				$batal = "UPDATE buku_induk set ".
				"c1 = '',".
				"c2 = '',".
				"d2 = '',".
				"e2 = '',".
				"e12 = '',".
				"no_ba2 = '',".
				"tgl_ba2 = NULL,".

